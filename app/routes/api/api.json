{
  "controllerPath": "controllers/api",
  "subdomain": "semote-bot",
  "routesGroups": [
    {
      "controller": "cemote.js",
      "prefix": "/cemote",
      "routes": [
        {
          "method": "post",
          "url": "/help",
          "handler": "help"
        },
        {
          "method": "post",
          "url": "/list",
          "handler": "list"
        },
        {
          "method": "post",
          "url": "/search",
          "handler": "search"
        },
        {
          "method": "post",
          "url": "/",
          "handler": "index"
        }
      ]
    },
    {
      "controller": "index.js",
      "prefix": "/",
      "routes": [
        {
          "method": "get",
          "url": "/",
          "handler": "index"
        }
      ]
    }
  ]
}
